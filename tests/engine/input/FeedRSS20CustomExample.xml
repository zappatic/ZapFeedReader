<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0"
    xmlns:content="http://purl.org/rss/1.0/modules/content/"
    xmlns:torrent="http://xmlns.ezrss.it/0.1/">
    <channel>
        <title>Custom Example</title>
        <!-- guid sources -->
        <item>
            <content:encoded>test content</content:encoded>
            <title>title-as-guid</title>
        </item>
        <item>
            <description>content-as-guid</description>
        </item>
        <item>
            <!-- no guid source -->
        </item>
        <!-- enclosures -->
        <item>
            <enclosure url="https://example.com/file.jpg" length="100" type="image/jpeg" />
        </item>
        <item>
            <torrent:contentLength>100</torrent:contentLength>
            <torrent:magnetURI>magnet:link</torrent:magnetURI>
            <torrent:fileName>Legit_Linux_ISO.iso</torrent:fileName>
        </item>
        <item>
            <guid>http://example.com/guid</guid>
            <category>cat1</category>
            <category>cat2</category>
        </item>
    </channel>
</rss>